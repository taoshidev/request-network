{
  "name": "request-network",
  "version": "v1.0.2",
  "main": "dist/index.js",
  "description": "",
  "types": "dist/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taoshidev/request-network.git"
  },
  "keywords": [
    "Node.js",
    "ReqNet",
    "Taoshi"
  ],
  "author": "Ken Raleigh / Lucas Phan",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "start": "cross-env node -r tsconfig-paths/register dist/index.js",
    "dev": "pnpm build && nodemon - exec 'ts-node' src/app.ts",
    "build": "rm -rf dist && tsc && cp -r src/views dist/views && cp -r src/public dist/public",
    "deploy:staging": "pnpm build && eb deploy rn-staging --profile taoshi",
    "db:generate": "drizzle-kit generate:pg",
    "introspect:pg": "drizzle-kit introspect:pg",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.40.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "ejs": "^3.1.10",
    "ethers": "^6.11.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "postgres": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3",
    "winston": "^3.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.14",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
