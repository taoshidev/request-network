          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:secretsmanager:us-west-1:533267243769:secret:reqnet-staging-env-5HnRrd-NODE_ENV"
        },
        {
          "name": "API_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-west-1:533267243769:secret:reqnet-staging-env-5HnRrd-API_HOST"
        },
        {
          "name": "API_PORT",
