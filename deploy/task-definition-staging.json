{
  "family": "request-network-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::533267243769:role/ecs_assume_role",
  "containerDefinitions": [
    {
      "name": "request-network",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SECRET_MANAGER_ARN",
          "value": "arn:aws:secretsmanager:us-west-1:533267243769:secret:reqnet-staging-env-5HnRrd"
        }
      ],
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "${SECRET_MANAGER_ARN}-NODE_ENV"
        },
        {
          "name": "API_HOST",
          "valueFrom": "${SECRET_MANAGER_ARN}-API_HOST"
        },
        {
          "name": "API_PORT",
          "valueFrom": "${SECRET_MANAGER_ARN}-API_PORT"
        },
        {
          "name": "API_PREFIX",
          "valueFrom": "${SECRET_MANAGER_ARN}-API_PREFIX"
        },
        {
          "name": "REQUEST_NETWORK_UI_URL",
          "valueFrom": "${SECRET_MANAGER_ARN}-REQUEST_NETWORK_UI_URL"
        },
        {
          "name": "VALIDATOR_OUTPUT_SERVER_API_URL",
          "valueFrom": "${SECRET_MANAGER_ARN}-VALIDATOR_OUTPUT_SERVER_API_URL"
        },
        {
          "name": "UNKEY_VERIFY_URL",
          "valueFrom": "${SECRET_MANAGER_ARN}-5HnRrd-UNKEY_VERIFY_URL"
        },
        {
          "name": "TAOSHI_API_KEY",
          "valueFrom": "${SECRET_MANAGER_ARN}-5HnRrd-TAOSHI_API_KEY"
        },
        {
          "name": "TAOSHI_VALIDATOR_API_SECRET",
          "valueFrom": "${SECRET_MANAGER_ARN}-TAOSHI_VALIDATOR_API_SECRET"
        },
        {
          "name": "VALIDATOR_WALLET_PRIVATE_KEY",
          "valueFrom": "${SECRET_MANAGER_ARN}-VALIDATOR_WALLET_PRIVATE_KEY"
        },
        {
          "name": "UPHOLD_CLIENT_ID",
          "valueFrom": "${SECRET_MANAGER_ARN}-UPHOLD_CLIENT_ID"
        },
        {
          "name": "UPHOLD_CLIENT_SECRET",
          "valueFrom": "${SECRET_MANAGER_ARN}-UPHOLD_CLIENT_SECRET"
        },
        {
          "name": "ENCRYPTION_KEY",
          "valueFrom": "${SECRET_MANAGER_ARN}-ENCRYPTION_KEY"
        },
        {
          "name": "IV_STRING",
          "valueFrom": "${SECRET_MANAGER_ARN}-IV_STRING"
        },
        {
          "name": "INFURA_PROJECT_ID",
          "valueFrom": "${SECRET_MANAGER_ARN}-INFURA_PROJECT_ID"
        },
        {
          "name": "POSTGRES_HOST",
          "valueFrom": "${SECRET_MANAGER_ARN}-POSTGRES_HOST"
        },
        {
          "name": "POSTGRES_DB",
          "valueFrom": "${SECRET_MANAGER_ARN}-POSTGRES_DB"
        },
        {
          "name": "POSTGRES_PORT",
          "valueFrom": "${SECRET_MANAGER_ARN}-POSTGRES_PORT"
        },
        {
          "name": "POSTGRES_USER",
          "valueFrom": "${SECRET_MANAGER_ARN}-POSTGRES_USER"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "valueFrom": "${SECRET_MANAGER_ARN}-POSTGRES_PASSWORD"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "${SECRET_MANAGER_ARN}-DATABASE_URL"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/request-network-app",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
